{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-secure-store"
    ],
    "newArchEnabled": true,
    "name": "vwanu-app",
    "slug": "vwanu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icons-data_image/icon.png",
    "userInterfaceStyle": "light",

    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e550dc60-d34d-4c1d-8286-c670002c81d3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.waddprog.vwanuapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.waddprog.vwanuapp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components"
          ]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "e550dc60-d34d-4c1d-8286-c670002c81d3"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
